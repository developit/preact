{
  "presets": [
    [
      "@babel/env",
      {
        "loose": true,
        "exclude": [
          "transform-typeof-symbol"
        ],
        "targets": {
          "browsers": [
            "last 2 versions",
            "IE >= 9"
          ]
        }
      }
    ]
  ],
  "plugins": [
    "@babel/proposal-object-rest-spread",
    "@babel/transform-react-jsx"
  ]
}
